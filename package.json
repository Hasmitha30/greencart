  "dependencies": {
    "@prisma/client": "^5.8.0",
    "bcrypt": "^5.1.0",
    "body-parser": "^1.20.2",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "jsonwebtoken": "^9.0.0"
  },
  "devDependencies": {
    "prisma": "^5.8.0",
    "ts-node": "^10.9.1",
    "typescript": "^5.4.2",
    "nodemon": "^2.0.22",
    "jest": "^29.6.1",
    "ts-jest": "^29.1.0",
    "@types/express": "^4.17.17",
    "@types/node": "^20.6.6",
    "@types/jsonwebtoken": "^9.0.1",
    "@types/bcrypt": "^5.0.0"
  }
}