{
  "name": "greencart-backend",
  "version": "0.1.0",
  "private": true,
  "main": "dist/index.js",
  "scripts": {
    "build": "tsc",
    "dev": "nodemon --watch 'src/**/*' --exec ts-node src/index.ts",
    "start": "node dist/index.js",
